    <% if (isMobile) { %>
      <section class="deep-link">
        <a href="bpe://bpe?token=<%= token %>&domain=<%= encodeURIComponent(process.env.DOMAIN) %>">
          <img src="/images/bpe-logo.png">
          <p>Add photos</p>
        </a>
      </section>
    <% } else { %>
      <section>
        <p>This web app is augmented with a native Android app. Login from your tablet or browser to send photos.</p>
      </section>
    <% } %>
