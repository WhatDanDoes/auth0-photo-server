<% include _partials/head %>
  <% include _partials/messages %>
  <main>
    <% if (!images.length) { %>
      <h2>No images</h2>
    <% } else { %>
      <% include _partials/pager %>
      <% for (let image of images) { %>
        <article class="post">
          <% if (agent) { %>
            <header>
              <img class="avatar" src="<%= image.photographer.get('picture') %>" />
              <aside>
                <div><%= image.photographer.get('nickname') %></div>
                <time><%= image.updatedAt %></time>
              </aside>
            </header>
          <% } %>
          <section class="photo">
            <img src="/<%= image.path %>" />
          </section>
        </article>
      <% } %>
      <% include _partials/pager %>
    <% } %>
  </main> 
<% include _partials/footer %>

