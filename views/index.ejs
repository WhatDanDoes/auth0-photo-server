<% include _partials/head %>
  <% include _partials/messages %>
  <main>
    <% if (!images.length) { %>
      <h2>No images</h2>
    <% } else { %>
      <% include _partials/pager %>
      <% for (let image of images) { %>
        <article class="post">
          <% if (agent) { %>
            <% include image/_header %>
          <% } %>
          <section class="photo">
          <% if (agent) { %>
            <a href="/<%= image.path.replace('uploads', 'image') %>">
              <img src="/<%= image.path %>" />
            </a>
          <% } else { %>
            <img src="/<%= image.path %>" />
          <% } %>
          </section>
          <% if (agent) { %>
            <% include image/_feedbackControls %>
          <% } %>
        </article>
      <% } %>
      <% include _partials/pager %>
    <% } %>
  </main>
<% include _partials/footer %>

